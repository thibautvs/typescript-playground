{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "noImplicitAny": true,
    "sourceMap": true,
    "removeComments": true,
    "strictNullChecks": true,
    "outDir": "./build/"
  },
  "include": [
    "app/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
